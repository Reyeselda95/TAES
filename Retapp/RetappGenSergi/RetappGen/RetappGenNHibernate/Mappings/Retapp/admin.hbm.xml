<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="RetappGenNHibernate"
		                   namespace="RetappGenNHibernate.EN.Retapp">
		    
		    
			
				<class name="AdminEN" table="Admin"> 
				  	
						<id name="Id" column="idAdmin" type="integer"> 
							<generator class="identity"/>
						</id>
			
					
					<property name="Usr" type="string"> 
						<column name="Usr"  not-null="true"/>
					</property>
					<property name="Pass" type="string"> 
						<column name="pass"  not-null="true"/>
					</property>
			
			
			
			
					
					
					
		
		 </class>	
		  
		  <query name="AdminENReadAllHQL">from AdminEN</query>
		</hibernate-mapping>
	